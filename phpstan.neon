includes:
	- vendor/jangregor/phpstan-prophecy/extension.neon

parameters:
    bootstrapFiles:
        - phpunit.bootstrap.php
    excludes_analyse:
        - %currentWorkingDirectory%/tests/assets
    level: 8
    inferPrivatePropertyTypeFromConstructor: true
    paths:
        - bin/
        - src/
        - tests/
    tmpDir: data
